<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.server.mapper.OrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.server.entity.Order">
        <id column="id" property="id" />
        <result column="yId" property="yId" />
        <result column="ddsj" property="ddsj" />
        <result column="price" property="price" />
        <result column="sc" property="sc" />
        <result column="ze" property="ze" />
        <result column="zfzt" property="zfzt" />
        <result column="bz" property="bz" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, yId, ddsj, price, sc, ze, zfzt, bz
    </sql>

</mapper>
